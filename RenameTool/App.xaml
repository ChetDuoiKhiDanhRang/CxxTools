<Application x:Class="RenameTool.App"
             xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
             xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
             xmlns:local="clr-namespace:RenameTool"
             StartupUri="MainWindow.xaml">
    <Application.Resources>
        <BitmapImage x:Key="WindowsIcon" UriSource="./imgs/icon24.png"/>
        <BitmapImage x:Key="btnIcon" UriSource="./imgs/tasks.png"/>
        <BitmapImage x:Key="FolderIcon" UriSource="./imgs/folder.png"/>
        <BitmapImage x:Key="FileIcon" UriSource="./imgs/file.png"/>
        <SolidColorBrush x:Key="ErrorColor" Color="#e81b2c"/>
        <!--<properties:Settings x:Key="AppSettings"/>-->
    </Application.Resources>
</Application>
